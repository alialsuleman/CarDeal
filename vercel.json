{
    "version": 2,
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/server.js"
        },
        {
            "src": "/public/(.*)",
            "dest": "public/$1"
        }
    ]
}